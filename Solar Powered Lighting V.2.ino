float operateModeFlag(float solarPower, int chargeFlag, int dischargeFlag, int errorFlag, float soc_batt){

  dod_batt = 1-soc_batt;
  
  if (solarPower > 0 && soc_batt<=1 && soc_batt>=0.5) {
    chargeFlag = 1;
    dischargeFlag = 0;
    errorFlag = 0;
   } else if(solarPower > 0 && soc_batt>1){
    chargeFlag = 0;
    dischargeFlag = 0;
    errorFlag = 0;
   } else if (solarPower == 0 && dod_batt <= 0.5){
    chargeFlag = 0;
    dischargeFlag = 1;
    errorFlag = 0;
   } else if (solarPower == 0 && dod_batt > 0.5){
    chargeFlag = 0;
    dischargeFlag = 0;
       
   } else {
    chargeFlag = 0;
    dischargeFlag = 0;
    errorFlag = 1;
 }
   return  chargeFlag, dischargeFlag, errorFlag;
}

float chargeCurrCal(float solarPower, float chargeCurr, float v_batt, float soc_batt, int errorFlag){
  if (soc_batt<=0.7&& soc_batt >= 0.8){
  chargeCurr = C1_chargeCurr;
  errorFlag = 0;
  } else if (soc_batt > 0.8 && soc_batt<=1){
     v_batt = 6;
     chargeCurr = solarPower/v_batt;
     errorFlag = 0;
  } else {
    chargeCurr = 0;
    errorFlag = 1;
  }
  return chargeCurr, errorFlag; 
}


float dischargeCurrCal(float dischargeCurr, float v_batt, float soc_batt, int errorFlag){
  dod_batt = 1-soc_batt;
  if (dod_batt>=0&& dod_batt <= 0.2){
    v_batt = 6;
    dischargeCurr = p_lamp/v_batt;
    if (dischargeCurr>dischargeCurr_max){
      dischargeCurr = dischargeCurr_max;
      errorFlag = 0;
    } else if(dod_batt>0.2&& dod_batt <= 0.4){
    v_batt = 0.8*(6);
    dischargeCurr = p_lamp/v_batt;
    errorFlag = 0;
  } else if (dod_batt>0.4&& dod_batt <=0.5){
    v_batt = 0.4*(6);
    dischargeCurr = p_lamp/v_batt;
    errorFlag = 0;
  } else{
    dischargeCurr = 0;
    errorFlag = 1;
  }
   return dischargeCurr, errorFlag;
}
